
{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-central1"
    },
    "rewrites": [
      {
        "source": "/api/**",
        "run": {
          "serviceId": "strategist-api-service",
          "region": "us-east4"
        }
      },
      {
        "source": "/__/firebase/init.json",
        "destination": "/__/firebase/init.json"
      },
      {
        "source": "/__/firebase/**",
        "destination": "/__/firebase/init.json"
      },
      {
        "source": "**",
        "function": "server"
      }
    ]
  }
}
